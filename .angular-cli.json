{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular2-material-design"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.less",
        "./libs/css/bootstrap.min.css",
        "./libs/css/bootstrap-responsive.min.css",
        "./libs/css/font-awesome.min.css",
        "./libs/css/style-metro.css",
        "./libs/css/style.css",
        "./libs/css/style-responsive.css",
        "./libs/css/default.css",
        "./libs/css/uniform.default.css",
        "./libs/css/DT_bootstrap.css",
        "./libs/css/jquery.gritter.css",
        "./libs/css/daterangepicker.css",
        "./libs/css/fullcalendar.css",
        "./libs/css/jqvmap.css",
        "./libs/css/jquery.easy-pie-chart.css",
        "./libs/css/chosen.css",
        "./libs/css/error.css"
      ],
      "scripts": [
        "./libs/js/jquery-1.10.1.min.js",
        "./libs/js/jquery-migrate-1.2.1.min.js",
        "./libs/js/jquery-ui-1.10.1.custom.min.js",
        "./libs/js/bootstrap.min.js",
        "./libs/js/jquery.slimscroll.min.js",
        "./libs/js/jquery.blockui.min.js",
        "./libs/js/jquery.cookie.min.js",
        "./libs/js/jquery.uniform.min.js",
        "./libs/js/jquery.vmap.js",
        "./libs/js/jquery.vmap.russia.js",
        "./libs/js/jquery.vmap.world.js",
        "./libs/js/jquery.vmap.europe.js",
        "./libs/js/jquery.vmap.germany.js",
        "./libs/js/jquery.vmap.usa.js",
        "./libs/js/jquery.vmap.sampledata.js",
        "./libs/js/jquery.flot.js",
        "./libs/js/jquery.flot.resize.js",
        "./libs/js/jquery.pulsate.min.js",
        "./libs/js/date.js",
        "./libs/js/daterangepicker.js",
        "./libs/js/jquery.gritter.js",
        "./libs/js/fullcalendar.min.js",
        "./libs/js/jquery.easy-pie-chart.js",
        "./libs/js/jquery.sparkline.min.js",
        "./libs/js/app.js",
        "./libs/js/index.js",
        "./libs/js/angular.js",
        "./libs/js/angular-resource.min.js",
        "./libs/js/angular-route.min.js",
        "./libs/js/jcs-auto-validate.min.js",
        "./libs/js/bootstrap_datepicker.min.js",
        "./libs/js/bootstrap_datepicker.zh-CN.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
